# Hive JDBC Connection String
jdbc.url=jdbc:hive2://localhost:10000/default
jdbc.user=
jdbc.password=
# File with Hive query used to generate split point list
query=/conf/query.sql

# Destination Phoenix Table
hbaseTable=EXAMPLE
# Number of salt buckets defined in Phoenix target table's DDL
saltBuckets=5

hbaseConfDir=/etc/hbase/conf
hadoopConfDir=/etc/hadoop/conf

kerberized=true
krb5Conf=/etc/krb5.conf
krb5Debug=false
hbasePrincipal=hbase
hbaseKeytab=/etc/security/keytabs/hbase.headless.keytab
